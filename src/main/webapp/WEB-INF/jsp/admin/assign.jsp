<%@page pageEncoding="UTF-8"%><%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><!DOCTYPE html><html >  <%@include file="/WEB-INF/jsp/common/htmlBase.jsp"%>    <script type="text/javascript">  $(function () {	    $("#left2RightBtn").click(function(){	    	var opts = $("#leftList :selected");	    	if ( opts.length == 0 ) {	                     layer.msg($.i18n.prop('assign-msg1'), {time:2000, icon:5, shift:6}, function(){});	    	} else {	    			    		$.ajax({	    			type : "POST",	    			url  : "${APP_PATH}/admin/doAssign",	    			data : JSON.stringify($("#roleForm").serializeJSON()),                    async : false,                    datatype : 'json',                    contentType:"application/json",	    			success : function(result) {	    				if ( 100==result.code ) {	    					$("#rightList").append(opts);	                        layer.msg($.i18n.prop('assign-msg2'), {time:2000, icon:6}, function(){});	    				} else {	                        layer.msg($.i18n.prop('assign-msg3'), {time:2000, icon:5, shift:6}, function(){});	    				}	    			}	    		});	    	}	    });	    $("#right2LeftBtn").click(function(){	    	var opts = $("#rightList :selected");	    	if ( opts.length == 0 ) {	                     layer.msg($.i18n.prop('assign-msg4'), {time:2000, icon:5, shift:6}, function(){	                     		                     });	    	} else {	    		$.ajax({	    			type : "POST",	    			url  : "${APP_PATH}/admin/doUnAssign",	    			data : JSON.stringify($("#roleForm").serializeJSON()),                    async : false,                    datatype : 'json',                    contentType:"application/json",	    			success : function(result) {	    				if ( 100==result.code ) {	    					$("#leftList").append(opts);	                        layer.msg($.i18n.prop('assign-msg5'), {time:2000, icon:6}, function(){});	    				} else {	                        layer.msg($.i18n.prop('assign-msg6'), {time:2000, icon:5, shift:6}, function(){});	    				}	    			}	    		});	    			    	}	    });   });   </script>  <body>    <div class="container-fluid">      <div>			<ol class="breadcrumb">							<li><a href="${APP_PATH}/admin/index" class="i18n"  data-properties="admin-title" data-ptype="text"></a></li>			  <li class="active i18n"  data-properties="assign-msg7" data-ptype="text"></li>			</ol>			<div class="panel panel-default">			  <div class="panel-body">				<form id="roleForm" role="form" class="form-inline">				  <input type="hidden" name="adminId" value="${admin.id}">				  <div class="form-group">					<label for="exampleInputPassword1" class="i18n"  data-properties="assign-msg8" data-ptype="text"></label><br>					<select id="leftList" name="unassignRoleIds" class="form-control"  size="10" style="width:200px;overflow-y:auto;">                        <c:forEach items="${unassignRoles}" var="role">                        <option value="${role.id}">${role.name}</option>                        </c:forEach>                    </select>				  </div>				  <div class="form-group">                        <ul>                            <li id="left2RightBtn" class="btn btn-default glyphicon glyphicon-chevron-right"></li>                            <br>                            <li id="right2LeftBtn" class="btn btn-default glyphicon glyphicon-chevron-left" style="margin-top:20px;"></li>                        </ul>				  </div>				  <div class="form-group" style="margin-left:40px;">					<label for="exampleInputPassword1" class="i18n"  data-properties="assign-msg9" data-ptype="text"></label><br>					<select id="rightList" name="assignRoleIds" class="form-control"  size="10" style="width:200px;overflow-y:auto;">                        <c:forEach items="${assingedRoles}" var="role">                        <option value="${role.id}">${role.name}</option>                        </c:forEach>                    </select>				  </div>				</form>			  </div>			</div>        </div>    </div>          </body></html>